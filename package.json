    "framer-motion": "11.2.9",
    "is-ci": "3.0.1",
    "phone": "^3.1.43",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-highlight-words": "0.20.0",
    "react-responsive": "10.0.0",
    "react-router-dom": "6.23.1",
    "react-select": "5.8.0",
    "react-verification-input": "4.1.2",
    "validator": "13.12.0",
    "vite-plugin-node-polyfills": "0.22.0",
