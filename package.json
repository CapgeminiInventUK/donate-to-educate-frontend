  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}": [
      "npm run format:fix"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run lint"
    ]
  },
