  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "coverage": "jest --coverage",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src",
