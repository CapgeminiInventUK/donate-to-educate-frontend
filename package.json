{
  "name": "d2e",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage --watch=false",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src",
    "lint": "eslint src --report-unused-disable-directives",
    "prepare": "is-ci || husky",
    "codegen": "amplify codegen"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}": [
      "npm run format:fix"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@aws-amplify/geo": "^3.0.35",
    "@aws-amplify/ui-react-geo": "^2.0.16",
    "@eslint/compat": "^1.1.1",
    "@tanstack/react-query": "5.51.24",
    "@tanstack/react-query-devtools": "5.51.24",
    "antd": "5.20.2",
    "aws-amplify": "6.5.1",
    "framer-motion": "11.3.28",
    "globals": "^15.9.0",
    "is-ci": "3.0.1",
    "phone": "^3.1.43",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-highlight-words": "0.20.0",
    "react-responsive": "10.0.0",
    "react-router-dom": "6.26.1",
    "react-select": "5.8.0",
    "react-verification-input": "4.1.2",
    "validator": "13.12.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-node-polyfills": "0.22.0",
    "zod": "3.23.8",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.6.1",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-highlight-words": "0.20.0",
    "@types/react-test-renderer": "18.3.0",
    "@types/validator": "13.12.0",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "9.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-refresh": "0.4.10",
    "globals": "^15.9.0",
    "husky": "9.1.5",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "^24.1.0",
    "jsdom-worker": "^0.3.0",
    "lint-staged": "15.2.9",
    "msw": "^2.3.1",
    "prettier": "3.3.3",
    "react-test-renderer": "18.3.1",
    "sass": "1.77.8",
    "typescript": "5.5.4",
    "undici": "^6.18.2",
    "vite": "5.4.2",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^2.0.0",
    "vitest-dom": "^0.1.1"
  },
